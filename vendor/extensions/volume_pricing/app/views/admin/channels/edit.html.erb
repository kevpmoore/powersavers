<%= render :partial => 'admin/shared/configuration_menu' %>

<h1><%= t("editing_channel") %></h1>
<%= error_messages_for :channel %>

<% form_for(:channel, :url => object_url, :html => { :method => :put }) do |f| -%>
  <%= render :partial => "form", :locals => { :f => f, :channel_users => @channel_users } %>
  <p>
    <%=submit_tag t("update")%> or <%= link_to t("cancel"), admin_channels_path%>
  </p>
<% end %>